{
  "agent": {
    "internal": true,
    "debug": true
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/apollo/env/{apollo_env_alias}/var/output/logs/application.log.*",
            "log_group_name": "{apollo_env_full_name}/application.log",
            "log_stream_name": "{hostname}",
            "timezone": "UTC"
          }
        ]
      }
    }
  }
}